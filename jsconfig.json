{
 "compilerOptions": {
  "target": "esnext",
  "module": "esnext",
  "strict": true,
  "moduleResolution": "node",
  "experimentalDecorators": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "sourceMap": true,
  "baseUrl": "./",
  "paths": {
   "@/*": ["./src/*"]
  }
 },
 "include": ["src/**/*.vue", "src/**/*.js"]
}